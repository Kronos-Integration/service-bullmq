{
  "name": "@kronos-integration/service-bullmq",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": "./src/service-bullmq.mjs"
  },
  "description": "service around bullmq",
  "keywords": [
    "kronos-service"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme --section=API ./src/**/*.mjs",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/**/*.mjs"
  },
  "dependencies": {
    "@kronos-integration/service": "^10.4.32",
    "bullmq": "^1.89.1",
    "model-attributes": "^4.1.19"
  },
  "devDependencies": {
    "ava": "^4.3.1",
    "c8": "^7.12.0",
    "documentation": "^14.0.0",
    "semantic-release": "^19.0.4"
  },
  "engines": {
    "node": ">=16.16.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/service-bullmq.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/service-bullmq/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/service-bullmq#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-kronos-component"
    ]
  }
}
