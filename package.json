{
  "name": "@kronos-integration/service-bullmq",
  "version": "0.0.0-semantic-release",
  "sideEffects": false,
  "type": "module",
  "main": "./src/service-bullmq.mjs",
  "exports": {
    ".": "./src/service-bullmq.mjs"
  },
  "description": "service around bullmq",
  "keywords": [
    "kronos-service"
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/service-bullmq.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/service-bullmq.mjs"
  },
  "dependencies": {
    "@kronos-integration/service": "^9.2.18",
    "bullmq": "^1.8.7",
    "model-attributes": "^4.1.7"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.4",
    "documentation": "^13.0.2",
    "semantic-release": "^17.2.1"
  },
  "engines": {
    "node": ">=14.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/service-bullmq.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/service-bullmq/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/service-bullmq#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-github-action",
      "arlac77/template-kronos-component"
    ]
  }
}
